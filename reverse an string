#include <stdio.h>
#include <string.h>
#include<ctype.h>
int main()
{
    char a[100];
    int i=0;
    fgets(a,100,stdin);
    int len=strlen(a);
    char temp=0;
    if(a[len-1]=='\n'){
        a[len-1]='\0';
        len--;
    }
    for(int i=0;i<len/2;i++){
             temp=a[i];
            a[i]=a[len-1-i];
            a[len-1-i]=temp;
   }
     printf("%s\n",a);
    return 0;
}
