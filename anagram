#include <stdio.h>
#include <string.h>
#include<ctype.h>
int main()
{
    
    char a[100],b[100];
    scanf("%s %s",a,b);
    int c[26]={0};
    int d[26]={0};
    int l1=strlen(a);
    int l2=strlen(b);
    
    for(int i=0;i<l1;i++){
        int lower=tolower(a[i]);
        c[lower-'a']++;
    }
     for(int i=0;i<l2;i++){
        int lower=tolower(b[i]);
        d[lower-'a']++;
    }
   for(int i=0;i<26;i++){
    if(c[i]!=d[i]){
        printf("not a anagram");
        return 0;
    }
   }
   printf("it is a anagram");
    return 0;
}
